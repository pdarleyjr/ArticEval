{
  "name": "articulation-eval",
  "version": "1.0.0",
  "type": "module",
  "description": "Speech-Language Evaluation System",
  "scripts": {
    "start": "npx serve -s dist -l 3000",
    "dev": "npx serve . -l 3000",
    "build": "esbuild assets/js/main.js --bundle --outfile=dist/bundle.js --format=esm",
    "test-articulation": "npx serve . -l 3001",
    "test-articulation-node": "node test-articulation-info.js"
  },
  "dependencies": {
    "@lucia-auth/adapter-sqlite": "^3.0.2",
    "@types/bcryptjs": "^2.4.6",
    "bcryptjs": "^3.0.2",
    "formeo": "^3.1.3",
    "jspdf": "^3.0.0",
    "lucia": "^3.2.2",
    "serve": "^14.2.1"
  },
  "devDependencies": {
    "esbuild": "^0.25.5"
  },
  "serve": {
    "headers": [
      {
        "source": "**/*.js",
        "headers": [
          {
            "key": "Content-Type",
            "value": "text/javascript"
          }
        ]
      },
      {
        "source": "**/*.mjs",
        "headers": [
          {
            "key": "Content-Type",
            "value": "text/javascript"
          }
        ]
      }
    ]
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pdarleyjr/ArticEval.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pdarleyjr/ArticEval/issues"
  },
  "homepage": "https://github.com/pdarleyjr/ArticEval#readme"
}
